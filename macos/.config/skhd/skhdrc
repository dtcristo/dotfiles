# Open new iTerm window
alt - return : osascript -e 'tell application "iTerm2" to create window with default profile'

# Open new browser window
alt - r : osascript -e 'tell application "System Events"' -e 'tell process "Firefox" to click menu item "New Window" of menu "File" of menu bar 1' -e 'end tell'

# Open new Finder window
alt - t : osascript -e 'tell app "Finder" to make new Finder window'

# Relaunch Amethyst
alt + shift - x : osascript -e 'quit app "Amethyst"' && osascript -e 'open app "Amethyst"'

# Disable Hide (Cmd + H)
cmd - h :
